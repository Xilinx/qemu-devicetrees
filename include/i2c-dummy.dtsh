#ifndef I2C_DUMMY_DTSH
#define I2C_DUMMY_DTSH

/* Dummy model for un-modelled i2c devices.  */
#define I2C_DUMMY(name, addr)			\
	name@addr {				\
		compatible = "i2c-dev-dummy";	\
		reg = <addr>;			\
	}

#endif /* I2C_DUMMY_DTSH */
